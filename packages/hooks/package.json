    "@element-plus/locale": "workspace:*",
    "@element-plus/utils": "workspace:*",
    "@floating-ui/dom": "^1.0.1",
    "@popperjs/core": "npm:@sxzz/popperjs-es@^2.11.7",
    "@vueuse/core": "^9.1.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.2",
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "*",
    "clean-package": "^2.2.0"
  },
  "tsup": {
    "clean": true,
    "target": "es2018",
    "format": [
      "cjs",
      "esm"
    ]
