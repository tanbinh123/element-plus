{
  "name": "@element-plus/hooks",
  "version": "3.0.0",
  "description": "Element Plus composables",
  "homepage": "https://github.com/element-plus/element-plus#readme",
  "bugs": {
    "url": "https://github.com/element-plus/element-plus/issues"
  },
  "license": "MIT",
  "author": "Element Plus Team <hello@element-plus.org>",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "src/index.ts",
  "module": "src/index.ts",
  "unpkg": "index.js",
  "jsdelivr": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/element-plus/element-plus.git",
    "directory": "packages/hooks"
  },
  "scripts": {
